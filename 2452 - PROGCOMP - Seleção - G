op = int(input('1: adicao / 2: subtracao / 3: multiplicacao / 4: divisao\n'))
num1 = float(input())
num2 = float(input())

if op == 1:
    print('A adicao eh: %.2f' % (num1 + num2))
elif op == 2:
    print('A subtracao eh: %.2f' % (num1 - num2))
elif op ==3:
    print('A multiplicacao eh: %.2f' % (num1 * num2))
elif op == 4:
    print('A divisao eh: %.2f' % (num1 / num2))
