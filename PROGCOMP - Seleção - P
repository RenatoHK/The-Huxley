tipo = input()
anos = int(input())
salario = float(input())

if tipo == 'h' or tipo == 'm':
    if tipo == 'h' and anos > 15:
        print('%.2f' % (salario * 1.20))
    else:
        if tipo == 'm' and anos > 10:
            print('%.2f' % (salario * 1.25))
        else:
            print('%.2f' % (salario + 200))
